_header01.css
*****************************************/
#header01{
	.h_lead_area{background-color: $key-color;}
	.h_lead_txt{color: #ffffff;}
	.h_lead_nav{
		a{color: #ffffff;}
	}
	.h_lead_lang{
		a{
			color: $key-color;
			background-color: rgba(255,255,255,.6);
			border-color: rgba(255,255,255,.6);
			&.on,&:hover{
				background-color: $key-color;
				color: #ffffff;
				border-color: #ffffff;
			}
		}
	}
	.h_nav_area{background-color: #ffffff;}
	/* globalnavi PC ------------*/
	@media only screen and (min-width: 1050px) {
		#globalnav{
			background-color: $key-color;
			border-color: #cccccc;
		}
		#globalnav ul{
			border-color: rgba(255,255,255, 0.5);
			li{
				list-style: none;
				width: 100%;
				text-align: center;
				padding: 0 8px;
				border-right: 1px solid rgba(255,255,255, 0.5);
				a{
					background-color: $key-color;
					color: #ffffff;
					&:hover{
						background-color: rgba(255,255,255, 0.2);
					}
				}
			}
		}
		#globalnav > ul > li:hover > a{color: $key-color;}
		/* globalnavi メガメニュー ----*/
		#globalnav{
			ul.menu__second-level {
				&::before{border-color: transparent transparent #eeeeee transparent;}
			}
			.menu__second-level li a:hover{background-color: darken($key-color, 10%);}
			li.menu__single ul.menu__second-level {background-color: #eeeeee;}
			li.menu__single ul.menu__second-level li a:hover{background-color: darken($key-color, 10%);}
		}
		/* tel contact btn ----*/
		.aux_menu .h_nav_btn a{
			background-color: $cv-link-color;
			color: #ffffff;
			&:hover{
				background-color: $cv-hover-color;
				color: #ffffff;
			}
		}
		.aux_menu .h_nav_btn{
			&.h_nav_btn_l a{
				background-color: $cv-link-color;
				&:hover{background-color: $cv-hover-color;}
			}p

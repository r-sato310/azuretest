/****************************************
layout.css
*****************************************/
/* background ***/
.col_bg{background-color: $bg-color-base;}
.col_bg_pwide{background-color: $bg-color-base;}
.col_bg_white{background-color: #ffffff;}
.col_border{border-color: $border-color-base;}
.bg_next::after{border-color: $bg-color-base transparent transparent transparent;}



/****************************************
module.css
*****************************************/
/* txt position ***/
.flex-control-paging li a:hover,
.flex-control-paging li a.flex-active {background: $btn-link-color;}

/* list stylen ***/
.list_material li .material-icons{color: $key-color;}
.list_material_bg li{background-image: url("../image/ic_feedback_black_24dp_2x.png");}
.list_counter li.list_c::after{color: $key-color;}


/****************************************
sub.css
*****************************************/
/* 特徴　強み　選ばれる理由 企業理念 など ***/
.col_vm_area{background-color: #f9f9f9;}

@media only screen and (max-width: 979px) {
	.col_vm_area .col_vm_txt{background-color: #f9f9f9;}
}
/* 企業理念 ***/
.rinen{
	> p{
		background-color: #ffffff;
		&::after,&::before{background-color: #999999;}
	}
}
/*　faq QA　よくある質問 ***/
.qa_q{
	color: #fff;
	background-color: $key-color;
}
.qa_click_q{background-color: #f5f5f5;}
.qa_click_q::before{background-color: $key-color;}
.qa_click_q::after{border-color: #ffffff;}
.qa_click_q.active::after{border-color: #ffffff;}
.qa_q_simple,
.qa_a_simple{
	.init{
		color: $key-color;
		border-color: $key-color;
	}
}
.qa_a_simple{
	.init{
		color: #ffffff;
		border-color: $accent-color1;
		background-color: $accent-color1;
	}
}
@media only screen and (max-width:690px) {
	.qa_click_q::after{border-color: $key-color;}
	.qa_click_q.active::after{border-color: $key-color;}
}
/*　サイトマップ ***/
#sitemap a {color: #333333;}
#sitemap li::before {color: #aaaaaa;}
/* page contact ***/
.colcontact{background-color: $bg-color-base;}
/* side contact ***/
.sidecontact_in{
	background-color: $bg-color-base2;
}
/* form contact ***/
.formflow li:after{border-color: $key-color;}
.formflow li.prev{background-color: #dddddd;}
.formflow li.age{background-color: $key-color;}
.formflow li.next{background-color: $accent-color1;}
/* side search ***/
.sub_search{
	.search_txt{border-color: #eeeeee;}
	.submit_btn { background-image: url("../image/common/icon_search.png");}
}
.sub_search input {
	&.search_txt {
		background-color: #f5f5f5;
		color: #777777;
	}
	&.submit_btn {
		background-color: #555555;
		background-image: url("../image/common/icon_search.png");
	}
}
/* submain area A ***/
.sm_a_area{
	.sm_a_block{background-image: url("../image/sub_sample_bg_1000450.jpg");}
}
/* submain area B ***/
.sm_b_area{
	background-image: url("../image/sub_sample_bg_2000450.jpg");
	color: #ffffff;
}
.sm_b_block{background-color: rgba(162, 188, 14, 0.6);}

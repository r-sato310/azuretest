/****************************************
_title.css
*****************************************/

/* bese ***/
@mixin title02{background-color: $accent-color2;}
@mixin title02_in{color: #ffffff;}

/* pagetitle h1 ***/
#pagetitle01{
	background-color: $accent-color2;
	.pagetitle{	color: #ffffff;	}
}
#pagetitle02{
	background-color: $bg-color-base;
	.pagetitle{	color: $text-color;
		&::after{background-color: $key-color;}
	}
}
#pagetitle03{
	background-color: $bg-color-base;
	.pagetitle{
		color: $text-color;
		&::after{background-color: $key-color;}
	}
}
#pagetitle04,
.pagetitle04{
	background-image: url(../image/title02_bg.jpg);
	.pagetitle{	color: #ffffff;}
}
#pagetitle05{
	background-color: $accent-color2;
	.pagetitle{	color: #ffffff;}
}
#pagetitle06{
	background-color: $bg-color-base;
	.pagetitle{	color: $text-color;
		&::after{background-color: #DF0000;}
	}
}
#pagetitle07{
	background-color: $bg-color-base;
	.pagetitle{	color: $text-color;}
}
#pagetitle08{
	background-color: $bg-color-base;
	.pagetitle{	color: $text-color;	}
}

/* h2 ***/
.title03_bl{border-color: $title-color;}
.title03_blb{border-color: $title-color;}
.title03_blb2{border-color: $title-color;}
.title03_blb2:after{background-color: $title-color;}
.title03_bbl{border-color: $title-color;}
.title03_bbs:after{	background-color: $title-color;}
.title03_bb2{border-color: $title-color;}
.title03_bb2:after{background-color: $title-color;}
.title03_bg{
	background-color: $title-color;
	color: #ffffff;
}
.title03_bgbb{
	background-color: $title-color;
	border-color: $accent-color2;
	color: #ffffff;
}
.title03_tc_bbs:after{background-color: $title-color;}
.title03_tc_verticalbm:after{background-color: $title-color;}
.title03_tc_verticalbm .ttv{background-color: #ffffff;}
.title03_crossbar {
	.title_in {	background-color: #ffffff;}
	&::before {	border-color: #333333;}
}
.title03_icon{background-image: url("../image/title_icon.png");}

/* h3 ***/
.title04_bl{border-color: $title-color;}
.title04_bbl{border-color: $title-color;}
.title04_bbs:after{background-color: $title-color;}
.title04_bb2{border-color: $title-color;}
.title04_bb2:after{background-color: $title-color;}
.title04_bg{
	background-color: $title-color;
	color: #ffffff;
}
.title04_square:after{background-color: $title-color;}
.title04_crossbar {
	.title_in {background-color: #ffffff;}
	&::before {border-color: #333333;}
}
.title04_icon{background-image: url("../image/title_icon.png");}

/* h4 ***/
.title05_bl{border-color: $title-color;}
.title05_bbl{border-color: $title-color;}
.title05_bbs:after{background-color: $title-color;}
.title05_bb2{border-color: $title-color;}
.title05_bb2:after{background-color: $title-color;}
.title05_bg{
	background-color: $title-color;
	color: #ffffff;
}
.title05_square:after{background-color: $title-color;}
.title05_crossbar {
	.title_in {background-color: #ffffff;}
	&::before {border-color: #333333;}
}
.title05_icon{background-image: url("../image/title_icon.png");}




/****************************************
_table.css
*****************************************/

/* base ***/
th,td{border-color: #eeeeee;}

/* table 01 ***/
table.tab01{table-layout: fixed;}
table.tab01 th{
	background-color: $key-color;
	color: #ffffff;
}
table.tab01 th.th_sub{background-color: #999999;}

/* table 02 ***/
table.tab02 tr:nth-child(2n){background-color: $bg-color-base;}

/* table 03 ***/
table.tab03 tr:nth-child(2n){background-color: $bg-color-base;}
table.tab03 tr:nth-child(2n) th{background-color: $key-color;}
table.tab03 th{
	background-color: $key-color;
	color: #ffffff;
}

/* table 04 ***/
table.tab04 th{
	background-color: $key-color;
	color: #ffffff;
}
table.tab04 thead th:nth-child(2n){background-color: $key-color;}
table.tab04 tbody td:nth-child(2n){background-color: $bg-color-base;}

/* table 05 ***/
table.tab05 th{
	background-color: $key-color;
	color: #ffffff;
}

/* table 06 ***/
table.tab06 th{
	background-color: $key-color;
	color: #ffffff;
}
@media only screen and (max-width:690px) {
	table.tab06 tbody td::before{
		background-color: $key-color;
		color: #ffffff;
	}
}

/* table 07 ***/
table.tab07 thead {border-color: $key-color;}
table.tab07 thead th { color: $key-color;}
table.tab07 thead tr th,
table.tab07 thead tr td,
table.tab07 tbody tr th,
table.tab07 tbody tr td{ border-color: #CCCCCC;}


/* table 08 ***/
table.tab08 tbody tr:not(:last-child) th,
table.tab08 tbody tr:not(:last-child) td{border-color: #CCCCCC;}
table.tab08 th{color: $key-color;}
table.tab08 th::after{ background-color: #CCCCCC;}
@media only screen and (max-width: 690px) {
  table.tab08 tbody tr:not(:last-child) td{border-color: #CCCCCC;}
  table.tab08 tbody td{padding-bottom: 16px;}
  table.tab08 th{color: $key-color;}
}

/* table 09 ***/
table.tab09 th,
table.tab09 td{	border-color: #EEEEEE;}
table.tab09 tr:nth-child(2n){background-color: #f9f9f9;}

/* table 10 ***/
table.tab10 th,
table.tab10 td{	border-color: #dddddd;}
table.tab10 th{color: $key-color;}
table.tab10 th::after{color: $key-color;}
@media only screen and (max-width: 690px) {
	table.tab10 tr:first-child th{border-color: #dddddd;}
}


/****************************************
_subnav.css
*****************************************/
/* column2 ***/
#subnav.sb_col2 ul li a{border-color: #bbbbbb;}
#subnav.sb_col2 ul li a:hover{
	color: $text-link-color;
	background-color: #f9f9f9;
}
#subnav.sb_col2 ul li.current a{
	border-color: #bbbbbb;
	color: $text-link-color;
	background-color: #f9f9f9;
}
#subnav.sb_col2 ul li.current ul{border-color: #bbbbbb;}

#subnav_title.sb_col2 a{
	background-color: $btn-link-color;
	color: #ffffff;
}
#subnav_title.sb_col2 a:hover{background-color: $btn-hover-color;}

/* column1 01 ***/
#subnav.sb_col1_1 ul li a:hover{background-color: #f7f7f7;}
#subnav.sb_col1_1 ul li a::after{color: $key-color;}
#subnav.sb_col1_1 ul li a{border-color: #f3f3f3;}
#subnav.sb_col1_1 ul li.current a{
	background-color: #f7f7f7;
	&::after{color: $key-color;}
}
#subnav_title.sb_col1_1 a{
	background-color: #ffffff;
	color: $key-color;
}
#subnav_title.sb_col1_1 a::before{background-color: #dddddd;}

@media only screen and (max-width:690px) {
	#subnav.col1_1	ul li a::before,
	#subnav.col1_1	ul li.current a::before{color: $key-color;}
}

/* column1 02 ***/
.sb_col1_2_wrap{background-color: #eeeeee;}
#subnav_title{
	border-color: #cccccc;
	a:hover{color: $key-color;}
}
#subnav_list li{
	border-color: #cccccc;
	&.current a,a:hover{
		color: $key-color;
		text-decoration: none;
	}
}


/****************************************
_media.css
*****************************************/
/* standard ***/
.media .media_in .catelist a{
	background-color: $key-color;
	color: #ffffff;
}
/* grid 採用TOP ***/
.grid_media_title{
	background-color: #999999;
	color: #ffffff;
}
.grid_media_con{background-color: #f9f9f9;}
.grid_media{
	border-color: #ffffff;
	background-color: #f9f9f9;
}
.grid_media_txt .catelist a{
	color: #ffffff;
	background-color: #555555;
}
@media only screen and (max-width: 690px) {
	.grid_media_section .slick-prev::before,
	.grid_media_section .slick-next::before{
		color: #000000;
	}
}


/****************************************
blog_layout.css
*****************************************/
/* common ***/
time{color: #555555;}
article#main a {
	color: $text-link-color;
	text-decoration: underline;
}
article#main .btn a,
article#main .btn_bg a,
article#main .catelist a,
article#main .taglist a {
	text-decoration: none;
}
article#main .btn_bg a,
article#main .catelist a,
article#main .taglist a,
article#main .btn a:hover,
article#main .btn_bg a:hover,
article#main .catelist a:hover,
article#main .taglist a:hover {
	color: #ffffff;
}

/* article list ***/
article{
	border-color: #dddddd;
	.info time{color: #000000;}
}
/* SNS ***/
.sns_share{
	/*&.head{	border-color: #f5f5f5;}*/
	&.foot{	border-color: #dddddd;}
	.line a{
		background-color: #00c300;
		color: #ffffff !important;
		text-decoration: none !important;
	}
}
/* next prev ***/
#nextprev li a:hover{color: $accent-color1;}

/* contact ***/
.contact{
	.title{
		color: #ffffff;
		background-color: $accent-color1;
	}
	.txt{
		border-color: #eeeeee;
		div{
			&.tel span{background-image: url(../image/common/icon_tel.png);}
			&.mail a{
				background-color: $btn-link-color;
				color: #ffffff;
				.in{background-image: url(../image/common/icon_letter.png);}
			}
		}
	}
}
/* relation ***/
#relation .title{border-color: #eeeeee;}
.relationlist time{color: #777777;}

/* article main title ***/
/*BMの見出しとあわせてください。*/
article#main{
	h2{border-color: $title-color;}
	h3{border-color: $title-color;}
	h4{}
	h5{}
}


/****************************************
blog_navi.css
*****************************************/
/* category navi common header ***/
.catenavi{
	border-color: #eeeeee;
	.catenavi_in li a{
		&:after{background-color: $text-link-color;}
		&:hover{color: $text-link-color;}
	}
}
/* search ***/
.blog_search {
	border-color: #eeeeee;
	input {
		&.submit_btn{
			background-image: url(../image/common/icon_search.png);
			background-color: $text-link-color;
		}
	}
}
/* side ***/
.side{
	.title{
		background-color: $title-color-bg;
		color: #ffffff;
	}
	.cate li a{
		color: #ffffff;
		background-color: $btn-link-color;
		&:hover{background-color: $btn-hover-color;}
	}
	.tag li a{
		background-image: url(../image/common/icon_sharp.png) ;
		color: $text-link-color;
	}
	.btn{
		background-color: #fafafa;
		@include box_bd;
		color: $text-link-color;
		&:hover{
			background-color: $text-link-color;
			color: #ffffff;
			span:before{border-color: #ffffff;}
		}
	}
	.sweep{border-color: #eeeeee;}
}
/* blog list category ***/
.catelist,.taglist a{color: $accent-color1;}
.taglist a{
	color: $bg-color-base;
	background-color: $accent-color3;
	&:before{color: $bg-color-base;}
}
.catelist a{
	color: $bg-color-base;
	background-color: $accent-color2;
	&:before{color: $bg-color-base;}
}
/* archive year month tag category list ***/
.archivelist{
	.list{
		a{
			border-color: $bg-color-base;
			color: #555555;
			&:hover,
			&.current{
				background-color: $accent-color3;
				color: #ffffff;
			}
		}
	}
}
/* pager new old ***/
.newold a,.pager a{color: $text-link-color;}
.pager span{
	background-color: $accent-color3;
	color: #ffffff;
}



/****************************************
common.css
*****************************************/
/* pagetop ***/
a.pagetop {background-image: url(../image/common/pagetop.gif);}

/* ぱんくず locator ***/
#locator_t_under,
#locator_t_up{
	background-color: #f5f5f5;
	border-color: $bg-color-base;
}


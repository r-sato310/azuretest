.banner_iconimg{
	.banner_in{
		a{
			background-color: $btn-link-color;
			color: #ffffff;
			&:hover{background-color: $btn-hover-color;}
		}
	}
}
.banner_bgimg .banner_in a{
	background-color: rgba(6,101,152,.8);
	color: #ffffff;
	&:hover{background-color: rgba(6,101,152,1);}
}
.banner_bgimg .bigglink:hover a{background-color: rgba(6,101,152,1);}
.sidefix_banner li a{
	background-color: $text-link-color;
	color: #ffffff;
}
.sidefix_banner li.sfb_1 a{	background-color: $accent-color1;}
.sidefix_banner li.sfb_2 a{	background-color: $accent-color2;}
.sidefix_banner li.sfb_3 a{	background-color: $accent-color3;}

/* sub area T ***/
.sub_t_area,.col,
.col:nth-child(3n+1),
.col:nth-child(5n+1){border-color: #dddddd;}
.sub_t_area .col{ position: relative; background-color: #ffffff;}
.sub_t_area .col.bl-hover{
	box-shadow: 0 0 8px 3px rgba(0, 0, 0, 0.1);
	z-index: 1;
}
.sub_t_area_m .col{background-color: #ffffff;}
.sub_t_area_m .col.bl-hover{
	box-shadow: 0 0 8px 5px rgba(0, 0, 0, 0.1);
	z-index: 1;
}
/****************************************
cv.css
*****************************************/
/* base ***/
@mixin cv_base{background-color: #f9f9f9;}
@mixin cv_telnum{
	color: $key-color;
	a{color: $key-color;}
}
@mixin cv_btn{
	color: #ffffff;
	background-color: $btn-link-color;
	&:hover{background-color: $btn-hover-color;}
}
@mixin cv_btn_rev{
	background-color: $btn-hover-color;
	&:hover{background-color: $btn-link-color;}
}

.cv_style01,.cv_style02,.cv_style03,.cv_style04{background-color: #f9f9f9;}

/* CV style 1 ***/
.cv_style01{background-image: url(../image/common/footer_contact_bg.jpg);}


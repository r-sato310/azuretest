@charset "utf-8";

/* ===================================================================
CSS information
 file name  :  color_blue.css
 style info :  カラー（青ベース） cssの定義
=================================================================== */

//色
$base-color: #ffffff; //white

$key-color: #329bd2; //blue(keycolor)
$accent-color1: #3DBEDE; //light blue
$accent-color2: #066598; //dark blue
$accent-color3: #FFC732; //yellow

$text-color: #333333; //black
$text-storong-color: #111111; //black
$text-link-color: #329bd2; //blue(keycolor)

$btn-link-color: #329bd2; //blue(keycolor)
$btn-hover-color: darken($btn-link-color, 10%); //dark
$cv-link-color: #FFA132; //blue(keycolor)
$cv-hover-color: darken($cv-link-color, 10%); //dark
$cv-link-color_d: darken($cv-link-color, 15%); //dark
$cv-hover-color_d: darken($cv-link-color, 20%); //dark

$title-color: #329bd2; //blue(keycolor)
$title-color-bg: #329bd2; //blue(keycolor)

$bg-color-base: #f5f5f5; //light gray
$bg-color-base2: #EBF0F7; //light blue
$border-color-base: #cccccc; //light gray



/****************************************
_setting.css
*****************************************/
/*.pb img,.pb1 img,.pb2 img{border-color: #999999;}*/
@mixin m_arr{border-color: $btn-link-color;}
@mixin box_bd{
    background-color: #fafafa;
    border-color: #eeeeee;
}


/****************************************
_button.css
*****************************************/
@mixin btn{
	border-color: $btn-link-color $btn-link-color $btn-link-color $btn-link-color;
	color: $btn-link-color;
	background-color: rgba(255,255,255,.7);
}
.btn a,.btn_half a,.btn_onethird a,.btn_twothird a,.btn_free a,.btn_fs_s a,.btn_circle a{@include btn;}
.btn a:hover,
.btn_half a:hover,
.btn_onethird a:hover,
.btn_twothird a:hover,
.btn_fs_s a:hover,
.btn_free a:hover{
	background-color: $btn-hover-color;
	color: #ffffff;
	border-color: $btn-hover-color;
}
.btn_bg a{
	text-decoration: none;
	background-color: $btn-link-color;
	color: #ffffff;
}
.btn_bg a:hover{background-color: $btn-hover-color;}

/* txt lilnk ***/
.txt_link a{color: $text-link-color;}

/* ページ内のナビゲーション ***/
.list_txt_link3,
.list_txt_link4{
	li{	border-color: $text-link-color;}
}
.list_txt_link3 a,
.list_txt_link4 a{
	&:hover{
		background-color: $text-link-color;
		color: #ffffff;
	}
}



/****************************************
_flow.css
*****************************************/

/* flow 01 ***/
.flow_ver01{
	.flow_item{
		background-color: $bg-color-base2;
		.finum{color: $key-color;
			&::after{background-color: $key-color;}
		}
		&::after{border-color: $accent-color3 transparent transparent transparent;}
	}
	.flow_txt{background-color: $bg-color-base;}
}
@media only screen and (max-width: 691px) {
	.flow_ver01{
		.flow_item .finum{background-color: rgba(255,255,255,.5);}
		&::after{border-color: $key-color transparent transparent transparent;}
	}
}

/* flow 02 ***/
.flow_txt_arr .txt{
	background-color: $key-color;
	color: #ffffff;
}
.flow_txt_arr:not(:last-child){
	.txt::after{border-color: transparent transparent transparent $key-color;}
}
.flow_txt_arr .detail{background-color: #f9f9f9;}
@media only screen and (max-width: 690px) {
	.flow_txt_arr:not(:last-child)::after{border-color: #999999 transparent transparent transparent;}
}

.flow_ver02_2 .flow_txt:not(:last-child)::after{border-color: transparent transparent transparent #999999;}
.flow_ver02_2 .flow_txt .txt{
	background-color: $key-color;
	color: #ffffff;
}
.flow_ver02_2 .flow_txt .detail{background-color: #f9f9f9;}

@media only screen and (max-width: 690px) {
	.flow_ver02_2 .flow_txt:not(:last-child)::after{border-color: #999999 transparent transparent transparent;}
}

/* flow 03 ***/
.flow_ver03{
	.flow_item{
		background-color: $bg-color-base2;
		.finum{color: $key-color;
			&::after{background-color: $key-color;}
		}
		&::after{border-color: $accent-color3 transparent transparent transparent;}
	}
	.flow_txt{border-color: $bg-color-base2;}
}
@media only screen and (max-width: 691px) {
	.flow_ver03{
		&::after{border-color: $key-color transparent transparent transparent;}
	}
}

/* 1日のスケジュール ***/

.schedule_line::before{	background-color: #dddddd;}
.schedule_line_time .item{
	color: #ffffff;
	background-color: #282F3A;
}
.schedule_line_txt {border-color: #eeeeee;}
.schedule_line_txt.leftside::after,
.schedule_line_txt.rightside::before{background-color: $key-color;}
.schedule_line_txt dt{color: $key-color;}
.schedule_line_txt_in{background-color: #ffffff;}

@media only screen and (max-width:690px) {
	.schedule_main_intro{background-color: #f9f9f9;}
	.schedule_line_txt.leftside::before{background-color: $key-color;}
}

/* flow 03 歴史 年表 ***/
.flow_ver03_tab::before{border-color: #333333;}


/* flow 04 採用 選考流れ ***/
.flow_ver04::before{background-color: #cccccc;}
.flow_ver04_num{
	background-color: $key-color;
	color: #ffffff;
}
.flow_ver04_d{border-color: #cccccc;}
.flow_ver04_info .info + .info{border-color: #999999;}



/****************************************
_news.css
*****************************************/

/* 基本設定 ***/
.rss_box dl{border-color: #eeeeee;}
